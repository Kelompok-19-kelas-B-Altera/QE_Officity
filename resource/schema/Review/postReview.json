{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "timestamp": {
            "type": "string"
        },
        "response_code": {
            "type": "integer"
        },
        "message": {
            "type": "string"
        },
        "data": {
            "type": "object",
            "properties": {
                "review": {
                    "type": "string"
                },
                "rating": {
                    "type": "integer"
                },
                "id_user": {
                    "type": "integer"
                },
                "id_building": {
                    "type": "integer"
                }
            },
            "required": ["review", "rating", "id_user", "id_building"]
        }
    },
    "required": ["timestamp", "response_code", "message", "data"]
}