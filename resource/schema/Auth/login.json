{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "timestamp": {
            "type": "string"
        },
        "response_code": {
            "type": "integer"
        },
        "message": {
            "type": "string"
        },
        "data": {
            "type": "object",
            "properties": {
                "token": {
                    "type": "string"
                }
            },
            "required": ["token"]
        }
    },
    "required": ["timestamp", "response_code", "message", "data"]
}